#!/usr/bin/perl

use strict;
use warnings;

use Data::Dumper;

use ForceField::Parameters;
use Grammar::ParameterParser;

$Data::Dumper::Sortkeys = 1;
$Data::Dumper::Indent = 1;

#
# Unit test: Grammar::ParameterParser.
# Example: ./parameter_parser '_[local]_force_field.t_k={id=1, resid=2}' parameters.cif
#

@ARGV = ( "-" ) unless @ARGV;
my ( $parameter_cmd, $parameter_file ) = @ARGV;

my $parameters = Parameters->new();

my $parameter_parser = new ParameterParser();
print Dumper $parameter_parser->parser( $parameter_cmd );
