#!/usr/bin/perl

use strict;
use warnings;

use lib "../../lib/perl";
use ConnectAtoms;

my @seq_of_numbers = map { split } 
                     grep { ! /\#/ } <>;

my ( $min_number, $max_number ) = ConnectAtoms::min_max( @seq_of_numbers );

print( join( " ", ( $min_number, $max_number ) ) . "\n" );
