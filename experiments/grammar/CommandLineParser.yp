%{
    use warnings;

    use PDBxParser qw( filter );
%}

%right '&' '|' '!'
%right '-'
%right ','

%%

line: exp
;

exp: exp
   | exp '&' exp
   | exp '|' exp
   | '!' exp
   | '(' exp ')'
   | 'resid' ope
   | 'atoid' ope
   | 'symbol' ope
   | 'atoname' ope
   | 'resname' ope
   | 'chain' ope
;

ope: ope ',' ope
   | ope '-' ope
   | num
   | str
;

%%
