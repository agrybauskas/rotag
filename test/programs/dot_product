#!/usr/bin/perl

use strict;
use warnings;

use Data::Dumper;

use lib "../../lib/perl";
use LinearAlgebra;

no warnings "experimental::autoderef";

my $symbols = shift;
my @symbols = [ split( ",", $symbols ) ];
my @matrices = ( [] );

while( <> ) {
    if( $_ =~ /^\n$/ ) {
	push( @matrices, [] );
    } else {
	push( $matrices[-1], [ split( " ", $_ ) ] );
    }
}

LinearAlgebra::dot_product( @symbols, @matrices );

