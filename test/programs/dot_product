#!/usr/bin/perl

use strict;
use warnings;

use Data::Dumper;

use lib "../../lib/perl";
use LinearAlgebra;

my @matrices = ( [] );

while( <> ) {
    if( $_ =~ /^\n$/ ) {
	push( @matrices, [] );
    } else {
	push( @{ $matrices[-1] }, [ split( " ", $_ ) ] );
    }
}

LinearAlgebra::dot_product( $matrices[0], $matrices[1] );
