#!/usr/bin/perl

use strict;
use warnings;

use lib "../../lib/perl";
use LinearAlgebra;

my @matrices = ( [] );

while( <> ) {
    if( $_ =~ /^\n$/ ) {
	push( @matrices, [] );
    } else {
	push( @{ $matrices[-1] }, [ split( " ", $_ ) ] );
    }
}

my $dot_product =
    LinearAlgebra::dot_product( $matrices[0], $matrices[1] );

foreach ( @$dot_product ) {
    print( join( "\t", @$_ ) . "\n" );
}
